{
  "name": "preact-boilerplate",
  "version": "2.0.0",
  "description": "Ready-to-go Preact starter project powered by webpack.",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --inline --progress",
    "start": "superstatic build -p ${PORT:-8080} --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "NODE_ENV=production webpack -p --progress",
    "prebuild": "mkdir -p build && ncp src/assets build/assets",
    "test": "eslint src tests/**/*.js"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "license": "MIT",
  "author": "Jason Miller <jason@developit.ca>",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "^6.5.2",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-minimal": "^1.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.5.0",
    "chai": "^3.5.0",
    "core-js": "^2.1.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "json-loader": "^0.5.4",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "ncp": "^2.0.0",
    "npm-run-all": "^1.5.1",
    "postcss-loader": "^0.8.1",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.1.5",
    "superstatic": "^4.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "decko": "^1.1.3",
    "preact": "^3.4.0",
    "preact-compat": "^0.7.1",
    "preact-router": "^1.2.3",
    "proptypes": "^0.14.3"
  }
}
